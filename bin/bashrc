#!/bin/bash
# Setup project context

export p=$HOME/Projects/Hammer

alias hammer=". $p/bin/bashrc"

cat $p/bin/welcome

# Path variable
export PATH=\
$p/bin:\
/bin:\
/sbin:\
/usr/bin:\
/usr/local/bin:\
/opt/local/bin

echo $PATH
export _OLD_VIRTUAL_PATH=$PATH


[ -f ~/Tools/env-python27/bin/activate ] &&
. ~/Tools/env-python27/bin/activate
echo "Active python = `which python`"
echo

cd $p

