#!/bin/bash
# Setup project context

export p=$HOME/Projects/MyBook
export PROJECT_BASE="$HOME/Projects/w2w"
export DROPLET_IP='45.55.50.13'
export SERVER_DIR=/home/django/MyBook

cat $p/bin/welcome

# Path variable
export PATH=\
$p/bin:\
/bin:\
/sbin:\
/usr/bin:\
/usr/local/bin:\
/opt/local/bin

echo 'PATH = ' $PATH
export _OLD_VIRTUAL_PATH=$PATH


[ -f ~/Tools/env-python27/bin/activate ] &&
. ~/Tools/env-python27/bin/activate
echo "Active python = `which python`"
echo


. $p/bin/bash-alias
. ~/bin/bash-prompt

cd $p

